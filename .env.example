# .env.example - 환경변수 예시 파일
# 실제 값은 입력하지 말고, 예시 형태로만 작성하세요

# Notion API 토큰 (https://www.notion.so/my-integrations에서 생성)
NOTION_TOKEN=your_notion_integration_token_here

# 헬스체크용 토큰 (선택사항)
HEALTH_CHECK_TOKEN=your_health_check_token_here

# Railway 배포시 자동 설정됨
PORT=8000

# CORS 설정
CORS_ORIGINS=*
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS,PATCH
CORS_HEADERS=Content-Type,Authorization,X-API-Key,X-Requested-With,Accept,Origin

# 환경 구분
ENVIRONMENT=production
